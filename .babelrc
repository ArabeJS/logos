{
  "presets": [
    [
      "env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "ie >= 10",
          "ie_mob >= 10",
          "ff >= 46",
          "chrome >= 52",
          "safari >= 7",
          "opera >= 42",
          "ios >= 7",
          "android >= 4.4",
          "bb >= 10"
        ]
      },
      "modules": false
    }
    ],
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }
    ]
  ],
  "compact": "true",
  "env": {
    "production": {
      "plugins": [
        "transform-flow-strip-types",
        "transform-decorators-legacy",
        "transform-object-assign",
        [
          "transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }
        ]
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "plugins": [
        "dynamic-import-node",
        "transform-es2015-modules-commonjs"
      ],
      "sourceMaps": "both"
    }
  }
}
